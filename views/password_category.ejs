<%- include('header')%>
        
    <h1>Password Category List</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Password Categroy Name</th>
        <th>Action</th>
      </tr>
    </thead>
    <% if(records.length>0){ 
      records.forEach(function(row){%>
    <tr>
      <td><%= row.password_category %></td>
      <td><a href="/passwordcategory/edit/<%= row._id%>" class="btn btn-primary btn-xs"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
        <a href="/passwordcategory/delete/<%= row._id%>"  class="btn btn-danger btn-xs"><i class="fa fa-trash" aria-hidden="true"></i></a></td>
      </tr>
     <% })} %>
  </table>
  <%-include('footer')%>
